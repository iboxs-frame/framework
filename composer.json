{
    "name": "iboxs/framework",
    "description": "The iboxsPHP Framework.",
    "keywords": [
        "framework",
        "iboxsphp",
        "ORM"
    ],
    "homepage": "http://www.lyweb.com.cn/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "itlattice",
            "email": "itlattice@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "ext-mbstring": "*",
        "psr/log": "^1.0|^2.0|^3.0",
        "psr/simple-cache": "^1.0|^2.0|^3.0",
        "psr/http-message": "^1.0",
        "iboxs/iboxs-orm": "*",
        "iboxs/iboxs-helper": "*"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/psr7": "^2.1.0"
    },
    "autoload": {
        "files": [],
        "psr-4": {
            "iboxs\\": "src/iboxs/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iboxs\\tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
